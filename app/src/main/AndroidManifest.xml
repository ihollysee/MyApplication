<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.holly.user.activity">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
        <activity
                android:name="com.holly.main.MainActivity"
                android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>


        <activity
                android:name="com.holly.inject.InjectActivity"
                android:label="@string/app_name">
        </activity>
        <activity
                android:name="com.holly.webview.WebViewActivity"
                android:label="@string/title_activity_web_view">
        </activity>
        <activity
                android:name="com.holly.horizontallistview.HorizontalListViewActivity"
                android:label="@string/title_activity_web_view">
        </activity>
    </application>

</manifest>
